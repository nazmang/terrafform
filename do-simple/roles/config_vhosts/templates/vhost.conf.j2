server {
  listen {{ item.item.listen | default('80') }};

{% if item.item.server_name is defined %}
  server_name {{ item.item.server_name }};
{% endif %}
{% if item.item.root is defined %}
  root {{ item.item.root }};
{% endif %}
  index index.html;

  location / {
    try_files $uri $uri/ =404;    
  } 
}
